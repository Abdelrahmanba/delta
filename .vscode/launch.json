{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug delta_compress",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/delta_compress",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",  // or "lldb" on macOS
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
        "ignoreFailures": true
        }
      ]
    }
  ]
}